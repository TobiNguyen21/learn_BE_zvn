<% const ad=systemConfig.prefixAdmin%>
    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1 class="m-0">
                        <%= title %>
                    </h1>
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item <%= (title=='Home') ? 'active':''%>">
                            <%- (title==='Home' ) ? 'Home' :`<a href="/${ad}">Home</a>` %>
                        </li>
                        <li class="breadcrumb-item <%= (title=='Dashboard') ? 'active':''%>">
                            <%- (title==='Dashboard' ) ? 'Dashboard' :`<a href="/${ad}/dashboard">Dashboard v1</a>` %>
                        </li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>