<% itemOrderingHelper=(id,ordering)=>{
    const ad = systemConfig.prefixAdmin;
    let func = `handleOrderingChange('${ad}','${id}','${ordering}')`;

    return `<input onchange="handleOrderingChange('${ad}','${id}','${ordering}')" type="number" value="${ordering}"
        class="text-center ordering" id="${id}_ordering">`;
    }
    %>